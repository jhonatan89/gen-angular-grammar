<?xml version="1.0" encoding="UTF-8"?>
<com.mtcflow.model:MTC xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:com.mtcflow.model="http://com.mtcflow.mtc/1.1">
  <files name="AngularApp"/>
  <metamodels name="MMWeb" alias="MMWeb" metamodelURI="http://www.academia.uniandes/Generator"/>
  <models name="App Model" URI="${model path}" metamodels="//@metamodels.0"/>
  <inputModels model="//@models.0" transformation="//@transformations.0"/>
  <transformations xsi:type="com.mtcflow.model:M2TTransformation" name="Model2Text" URI="main.egl" target="${generated path}"/>
  <configurations>
    <variables name="model path" defaultValue="../grammarBook.xmi"/>
    <variables name="generated path" defaultValue="gen/crudBeta2"/>
  </configurations>
  <outputFiles transformation="//@transformations.0" file="//@files.0"/>
</com.mtcflow.model:MTC>
